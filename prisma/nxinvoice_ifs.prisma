model IFS_Supplier_Document_Tax {
  id                     String        @id @default(dbgenerated("concat('ifs_tax_', gen_random_uuid())"))
  created_at             DateTime      @default(now()) @db.Timestamptz(6)
  updated_at             DateTime?     @db.Timestamptz(6)
  synchronized_at        DateTime?     @db.Timestamptz(6)
